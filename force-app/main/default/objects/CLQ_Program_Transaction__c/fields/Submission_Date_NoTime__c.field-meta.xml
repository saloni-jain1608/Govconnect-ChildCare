    <externalId>false</externalId>
    <formula>TEXT(MONTH(DATEVALUE(Submission_Date1__c)))+&quot;/&quot;+TEXT(DAY(DATEVALUE(Submission_Date1__c)))+&quot;/&quot;+TEXT(YEAR(DATEVALUE(Submission_Date1__c)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Submission Date with No Time Display</inlineHelpText>
    <label>Submission Date NoTime</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
