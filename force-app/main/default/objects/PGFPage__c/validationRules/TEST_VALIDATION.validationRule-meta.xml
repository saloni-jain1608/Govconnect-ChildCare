<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TEST_VALIDATION</fullName>
    <active>true</active>
    <errorConditionFormula>CONTAINS(Name, &apos;TESTING&apos;)</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Configurations Name should not contain TESTING</errorMessage>
</ValidationRule>
