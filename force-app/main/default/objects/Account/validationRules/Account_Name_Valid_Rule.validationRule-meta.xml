<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Name_Valid_Rule</fullName>
    <active>true</active>
    <errorConditionFormula>AND (ISCHANGED (Name ), CONTAINS($Profile.Name , &quot;Reviewer&quot;))</errorConditionFormula>
    <errorMessage>Cannot Edit Account Name</errorMessage>
</ValidationRule>
