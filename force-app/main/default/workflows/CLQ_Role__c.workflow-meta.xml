    <rules>
        <fullName>OCLQS_RoleSharingCounter</fullName>
        <active>true</active>
        <description>This workflow rule triggers to update sharing counter</description>
        <formula>$Setup.ExecutionControlSetting__c.Run_Workflow__c &amp;&amp; AND(  RecordType.Name ==&apos;Portal Roles&apos; , ISPICKVAL(Role_Type__c , &apos;JFS&apos;) ,  OR(CLQ_Start_Date__c &gt; Today(), CLQ_End_Date__c &gt; Today()) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>OCLQS_SharingCounter_Update</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>CLQ_Role__c.CLQ_Start_Date__c</offsetFromField>
            <timeLength>0</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>OCLQS_SharingCounter_Update</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>CLQ_Role__c.CLQ_End_Date__c</offsetFromField>
            <timeLength>1</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
